{
  "name": "form-management-api",
  "version": "1.0.0",
  "description": "Conversational forms backend API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node --experimental-specifier-resolution=node dist/index.js",
    "dev:watch": "concurrently \"tsc -w\" \"tsc-alias -w\" \"node --watch --experimental-specifier-resolution=node dist/index.js\"",
    "dev": "npm run build && node --experimental-specifier-resolution=node dist/index.js",
    "typeorm": "ts-node --esm ./node_modules/typeorm/cli.js",
    "test-connection": "ts-node --esm src/scripts/test-connection.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "prebuild": "npm run type-check",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^0.2.2",
    "@types/pg": "^8.11.11",
    "ai": "^4.2.8",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cron": "^4.3.0",
    "dotenv": "^16.4.7",
    "dotenv-safe": "^8.2.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "google-auth-library": "^9.15.1",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.0.5",
    "pg": "^8.14.1",
    "redis": "^4.7.0",
    "standardwebhooks": "^1.0.0",
    "typeorm": "^0.3.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.17.28",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "concurrently": "^9.1.2",
    "eslint": "^8.57.1",
    "glob": "^10.3.10",
    "globals": "^16.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.13",
    "tsc-files": "^1.1.3",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0"
  }
}
