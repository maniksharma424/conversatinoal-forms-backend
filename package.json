{
  "name": "form-management-api",
  "version": "1.0.0",
  "description": "Conversational forms backend API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node --experimental-specifier-resolution=node dist/index.js",
"dev:watch": "tsx watch src/index.ts",
    "dev": "npm run build && node --experimental-specifier-resolution=node dist/index.js",
    "typeorm": "ts-node --esm ./node_modules/typeorm/cli.js",
    "test-connection": "ts-node --esm src/scripts/test-connection.ts"
  },
  "dependencies": {
    "@ai-sdk/deepseek": "^0.2.2",
    "@types/pg": "^8.11.11",
    "ai": "^4.2.8",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "dotenv-safe": "^8.2.0",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "pg": "^8.14.1",
    "typeorm": "^0.3.21",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "^20.17.28",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/uuid": "^9.0.8",
    "glob": "^10.3.10",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.13",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
